{
  "$schema": "https://ui.shadcn.com/schema/registry.json",
  "name": "ai-tools",
  "homepage": "https://ai-tools-registry.vercel.app",
  "items": [
    {
      "name": "design-system",
      "type": "registry:style",
      "dependencies": ["radix-ui"],
      "registryDependencies": [
        "@ai-tools/button",
        "@ai-tools/input",
        "@ai-tools/calendar",
        "@ai-tools/card",
        "@ai-tools/label",
        "@ai-tools/popover",
        "@ai-tools/textarea"
      ],
      "cssVars": {
        "light": {
          "primary": "oklch(0.63 0.17 149)",
          "primary-foreground": "oklch(0.985 0 0)",
          "radius": "0.825rem"
        },
        "dark": {
          "primary": "oklch(0.63 0.17 149)",
          "primary-foreground": "oklch(0.985 0 0)"
        }
      },
      "files": [
        {
          "path": "registry/ai-tools/rules/AGENTS.md",
          "type": "registry:file",
          "target": "~/AGENTS.md"
        }
      ]
    },
    {
