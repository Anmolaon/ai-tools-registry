{
  "$schema": "https://ui.shadcn.com/schema/registry.json",
  "name": "ai-tools",
  "homepage": "https://ai-tools-registry.vercel.app",
  "items": [
    {
      "name": "design-system",
      "type": "registry:style",
      "dependencies": ["radix-ui"],
      "registryDependencies": [
        "@ai-tools/button",
        "@ai-tools/input",
        "@ai-tools/calendar",
        "@ai-tools/card",
        "@ai-tools/label",
        "@ai-tools/popover",
        "@ai-tools/textarea"
      ],
      "cssVars": {
        "light": {
          "primary": "oklch(0.63 0.17 149)",
          "primary-foreground": "oklch(0.985 0 0)",
          "radius": "0.825rem"
        },
        "dark": {
          "primary": "oklch(0.63 0.17 149)",
          "primary-foreground": "oklch(0.985 0 0)"
        }
      },
      "files": [
        {
          "path": "registry/ai-tools/rules/AGENTS.md",
          "type": "registry:file",
          "target": "~/AGENTS.md"
        }
      ]
    },
    {
      "name": "button",
      "type": "registry:ui",
      "title": "Button",
      "description": "A button component.",
      "files": [
        {
          "path": "registry/ai-tools/ui/button.tsx",
          "type": "registry:ui"
        }
      ]
    },
    {
      "name": "calendar",
      "type": "registry:ui",
      "title": "Calendar",
      "description": "A calendar component.",
      "dependencies": ["react-day-picker"],
      "files": [
        {
          "path": "registry/ai-tools/ui/calendar.tsx",
          "type": "registry:ui"
        }
      ]
    },
    {
      "name": "card",
      "type": "registry:ui",
      "title": "Card",
      "description": "A card component.",
      "files": [
        {
          "path": "registry/ai-tools/ui/card.tsx",
          "type": "registry:ui"
        }
      ]
    },
    {
      "name": "input",
      "type": "registry:ui",
      "title": "Input",
      "description": "An input component.",
      "files": [
        {
          "path": "registry/ai-tools/ui/input.tsx",
          "type": "registry:ui"
        }
      ]
    },
    {
      "name": "label",
      "type": "registry:ui",
      "title": "Label",
      "description": "A label component.",
      "files": [
        {
          "path": "registry/ai-tools/ui/label.tsx",
          "type": "registry:ui"
        }
      ]
    },
    {
      "name": "popover",
      "type": "registry:ui",
      "title": "Popover",
      "description": "A popover component.",
      "files": [
        {
          "path": "registry/ai-tools/ui/popover.tsx",
          "type": "registry:ui"
        }
      ]
    },
    {
      "name": "textarea",
      "type": "registry:ui",
      "title": "Textarea",
      "description": "A textarea component.",
      "files": [
        {
          "path": "registry/ai-tools/ui/textarea.tsx",
          "type": "registry:ui"
        }
      ]
    },
    {
      "name": "logo",
      "type": "registry:block",
      "title": "Logo",
      "description": "A logo component.",
      "files": [
        {
          "path": "registry/ai-tools/branding/logo.tsx",
          "type": "registry:component"
        }
      ]
    }
  ]
}
